pipeline {
    agent any
    triggers {
        pollSCM '* * * * *'
    }
    stages {
        stage('Build') {
            steps {

                dir('./eureka-service-discovery') {

                    withMaven(maven: 'Maven 3.9.2') {
                        bat 'mvn clean package'
                    }
                }
            }
        }
    }
}